{
  "name": "babel-ast-explorer",
  "displayName": "babel-ast-explorer",
  "description": "AST preview with @babel/parser",
  "icon": "icon.jpg",
  "publisher": "jyee721",
  "author": {
    "name": "Jennie Ji",
    "email": "jennie.ji@hotmail.com"
  },
  "version": "0.2.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:babelAstExplorer.start",
    "onCommand:babelAstExplorer.update",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "babelAstExplorer.start",
        "title": "Open",
        "category": "Babel AST Explorer",
        "icon": "$(triangle-right)"
      },
      {
        "command": "babelAstExplorer.update",
        "title": "Update",
        "category": "Babel AST Explorer"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "babelAstExplorer.start"
        },
        {
          "command": "babelAstExplorer.update",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "babelAstExplorer.start",
          "when": "view =~ /babelAstExplorer/",
          "group": "navigation@2"
        }
      ]
    },
    "views": {
      "babelAstExplorer": [
        {
          "id": "babelAstExplorer-plugins",
          "name": "Plugins"
        },
        {
          "id": "babelAstExplorer-options",
          "name": "Options"
        },
        {
          "id": "babelAstExplorer-versions",
          "name": "Babel Version"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "babelAstExplorer",
          "title": "Babel AST Explorer",
          "icon": "resources/icons/babel-logo-minimal.svg"
        }
      ]
    }
  },
  "auto-changelog": {
    "tagPrefix": "v",
    "ignoreCommitPattern": "^[^[]",
    "startingVersion": "v0.1.0"
  },
  "scripts": {
    "vscode:prepublish": "yarn fetch:parser && yarn changelog && yarn run compile",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "fetch:parser": "node ./scripts/fetchParsers.js",
    "compile": "rm -rf out && rollup -c",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/types": "^7.12.11",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.11.7",
    "@types/pacote": "^11.1.0",
    "@types/semver": "^7.2.0",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "auto-changelog": "latest",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "mocha": "^7.1.0",
    "pacote": "^11.1.10",
    "prettier": "^2.8.0",
    "rollup": "^3.12.0",
    "semver": "^7.3.2",
    "ts-node": "^8.10.1",
    "typescript": "^4.9.4",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JennieJi/vscode-babel-ast-explorer"
  },
  "bugs": {
    "url": "https://github.com/JennieJi/vscode-babel-ast-explorer/issues"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  }
}
